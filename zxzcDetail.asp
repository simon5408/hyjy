<html xmlns="http://www.w3.org/1999/xhtml">
	<head>
		<title>网上报名</title>
<!-- 头部公共操作类 -->
		<!--#include file="common/header.asp" -->
		
		<script type="text/javascript">
		
		</script>
</head>

<body>
	<!-- 头部操作菜单栏 -->
	<!--#include file="common/normalTop.asp" -->
    
	<!-- 广告标语区-start -->
    <!--#include file="common/ad.asp" -->
	<!-- 广告标语区-end -->
	
	<div id="gkPage">
		<div id="gkPageWrap" class="gkMain gkWrap">  
		    <div id="mainContent" class="clear">
				<div id="system-message-container"></div>
				
				<!-- -->
				<div id="gkTop2" class="gkMain clear">
					<!-- left -->
					<!--#include file="common/left.asp" -->
					<!-- right -->
					<div class="rightSide">
						<div class="newsTitle bgColor wsbmBgImg">
							<div class="phoneDiv">官方咨询电话：400-6565-800</div>
						</div>
						<div class="newsDetail">
							<div id="wsbmTab">
							<table cellpadding="0" cellspacing="0" border="0">
								<tr>
									<th>姓名：</th>
									<td><input type="text" name="zName" id="zName" style="width:180px;" /> <span>*</span></td>
									<td></td>
								</tr>
								<tr>
									<th>性别：</th>
									<td><input type="radio" name="zSex" value="1" checked="checked" /> 男 <input type="radio" name="zSex" value="0" /> 女 </td>
									<td></td>
								</tr>
								<tr>
									<th>电话：</th>
									<td><input type="text" name="zPhone" id="zPhone" style="width:180px;" /> <span>*</span></td>
									<td></td>
								</tr>
								<tr>
									<th>类别：</th>
									<td>
										<select name="zType" id="zType" style="width:185px;">
											<option value="0">--请选择--</option>
											<option value="1">自考学历</option>
											<option value="2">成人学历</option>
											<option value="3">远程学历</option>
											<option value="4">硕士研究生</option>
											<option value="5">专业证书</option>
											<option value="6">职业教育</option>
										</select> <span>*</span>
									</td>
									<td></td>
								</tr>
								<tr>
									<th>层次：</th>
									<td>
										<select name="zLevel" id="zLevel" style="width:185px;">
											<option value="0">--请选择--</option>
											<option value="1">高起专</option>
											<option value="2">高起本</option>
											<option value="3">专升本</option>
										</select> <span>*</span>
									</td>
									<td></td>
								</tr>
								<tr>
									<th style="margin-bottom:30px;">备注：</th>
									<td colspan="2"><textarea cols="80" rows="10" name="zIngo" id="zInfo"></textarea> <span>*</span></td>
								</tr>
							</table>
							</div>
							<div class="subBut">
								<div class="sDiv"><div class="buttonDiv">提 交</div> <div class="buttonDiv" style="margin-left:5px;">重 置</div></div>
							</div>
							<div class="bmxqTitle">&nbsp;报名详情</div>
							<div class="subBut">&nbsp;&nbsp;请各位同学如实填写各项内容，以免影响正常报名。 </div>
							<div class="subBut bottom_5">&nbsp;&nbsp;本中心未委托任何单位和个人代收学费，谨防上当受骗。</div>
						</div>
					</div>
					
				</div>
			</div>
		</div>
	</div>
<!-- 底部操作菜单栏 -->
	<!--#include file="common/footer.asp" -->

</body>

</html>
